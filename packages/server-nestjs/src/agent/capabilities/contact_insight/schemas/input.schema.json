{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "userId": {
      "type": "string"
    },
    "contactId": {
      "type": "string"
    },
    "depth": {
      "type": "string",
      "enum": ["brief", "standard", "deep"],
      "default": "standard"
    }
  },
  "required": ["userId", "contactId"]
}




