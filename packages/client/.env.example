# Local AI proxy configuration for /api/chat
LOCAL_AI_BASE_URL=http://127.0.0.1:9739/v1
LOCAL_AI_MODEL=claude-sonnet-4-5-thinking
LOCAL_AI_API_KEY=your-local-proxy-api-key

# Feishu Bitable sync (optional, disabled by default)
FEISHU_SYNC_ENABLED=false
# Server-side switch for /api/chat to accept feishu_template_message tool
FEISHU_CHAT_TOOL_ENABLED=false
FEISHU_APP_ID=
FEISHU_APP_SECRET=
FEISHU_BITABLE_APP_TOKEN=
FEISHU_BITABLE_TABLE_ID=
# JSON map from extracted keys to Bitable columns, e.g. {"Email":"邮箱","Company":"公司"}
FEISHU_FIELD_MAPPING_JSON={}
FEISHU_BASE_URL=https://open.feishu.cn
FEISHU_INTERNAL_API_TOKEN=your-internal-feishu-sync-token
